# Public ngrok tunnel URL (used to construct callbacks/endpoints)
NGROK_URL=your_ngrok_url_here

# OAuth2 client credentials (register these with your auth server)
OAUTH_CLIENT_ID=your_client_id_here
OAUTH_CLIENT_SECRET=your_client_secret_here

# Whether to verify SSL certificates on outgoing requests (False for dev/ngrok, True in prod)
REQUESTS_VERIFY=False

# Django secret key (keep this private!)
SECRET_KEY=your_django_secret_key_here
